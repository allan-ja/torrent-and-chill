{
  "name": "torrent-and-chill",
  "version": "0.1.0",
  "scripts": {
    "start": "node index.js",
    "client": "cd client && yarn start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.3"
  }
}
